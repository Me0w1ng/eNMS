{
  "engine": {
    "common": {
      "convert_unicode": true,
      "pool_pre_ping": true,
      "pool_recycle": 3600
    },
    "mysql": {
      "max_overflow": 10,
      "pool_size": 1000,
      "isolation_level": "READ COMMITTED"
    },
    "sqlite": {
      "connect_args": {
        "check_same_thread": false
      }
    }
  },
  "columns": {
    "length": {
      "tiny_string_length": 64,
      "small_string_length": 255,
      "large_string_length": 4294967295
    }
  },
  "transactions": {
    "retry": {
      "commit": {
        "number": 10,
        "time": 5
      },
      "fetch": {
        "number": 3,
        "time": 3
      }
    }
  },
  "import_export_models": [
    "user",
    "device",
    "link",
    "service",
    "network",
    "workflow_edge",
    "task",
    "access",
    "credential",
    "pool"
  ],
  "dont_migrate": {
    "access": ["id"],
    "credential": ["id"],
    "device": [
      "access",
      "id",
      "configuration",
      "operational_data",
      "specialized_data",
      "services",
      "source_id",
      "source",
      "destination",
      "destination_id",
      "pools",
      "networks",
      "target_services",
      "tasks",
      "users"
    ],
    "link": [
      "access",
      "id",
      "pools",
      "networks",
      "users",
      "destination_id",
      "destination_name",
      "source_id",
      "source_name"
    ],
    "pool": [
      "access",
      "access_users",
      "credential_devices",
      "credential_users",
      "device_number",
      "id",
      "link_number",
      "service_number",
      "services",
      "network_number",
      "user_number"
    ],
    "service": [
      "access",
      "children",
      "id",
      "sources",
      "destinations",
      "originals",
      "path",
      "status",
      "superworkflow_id",
      "tasks",
      "workflows",
      "tasks",
      "edges"
    ],
    "network": ["id", "path"],
    "task": [
      "access",
      "id",
      "service_name",
      "service_id",
      "next_run_time",
      "is_active",
      "time_before_next_run",
      "status"
    ],
    "user": [
      "access",
      "id",
      "devices",
      "pools",
      "links",
      "menu",
      "delete_requests",
      "get_requests",
      "post_requests",
      "services",
      "pages"
    ],
    "workflow_edge": ["id", "source_id", "destination_id", "workflow_id"]
  },
  "relationships": {
    "associations": {
      "device_gateway": {
        "model1": {
          "column": "device_id",
          "foreign_key": "device"
        },
        "model2": {
          "column": "gateway_id",
          "foreign_key": "gateway"
        }
      },
      "link_network": {
        "model1": {
          "column": "link_id",
          "foreign_key": "link"
        },
        "model2": {
          "column": "network_id",
          "foreign_key": "network"
        }
      },
      "node_network": {
        "model1": {
          "column": "node_id",
          "foreign_key": "node"
        },
        "model2": {
          "column": "network_id",
          "foreign_key": "network"
        }
      },
      "pool_device": {
        "model1": {
          "column": "pool_id",
          "foreign_key": "pool",
          "property": "pools"
        },
        "model2": {
          "column": "device_id",
          "foreign_key": "device",
          "property": "device"
        }
      },
      "pool_link": {
        "model1": {
          "column": "pool_id",
          "foreign_key": "pool"
        },
        "model2": {
          "column": "link_id",
          "foreign_key": "link"
        }
      },
      "user_group": {
        "model1": {
          "column": "group_id",
          "foreign_key": "group"
        },
        "model2": {
          "column": "user_id",
          "foreign_key": "user"
        }
      },
      "run_device": {
        "model1": {
          "column": "run_id",
          "foreign_key": "run",
          "kwargs": { "ondelete": "cascade" }
        },
        "model2": {
          "column": "device_id",
          "foreign_key": "device"
        }
      },
      "run_pool": {
        "model1": {
          "column": "run_id",
          "foreign_key": "run",
          "kwargs": { "ondelete": "cascade" }
        },
        "model2": {
          "column": "pool_id",
          "foreign_key": "pool"
        }
      },
      "service_device": {
        "model1": {
          "column": "service_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "device_id",
          "foreign_key": "device"
        }
      },
      "service_owner": {
        "model1": {
          "column": "service_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "user_id",
          "foreign_key": "user"
        }
      },
      "service_pool": {
        "model1": {
          "column": "service_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "pool_id",
          "foreign_key": "pool"
        }
      },
      "service_workflow": {
        "model1": {
          "column": "service_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "workflow_id",
          "foreign_key": "workflow"
        }
      },
      "task_device": {
        "model1": {
          "column": "task_id",
          "foreign_key": "task"
        },
        "model2": {
          "column": "device_id",
          "foreign_key": "device"
        }
      },
      "task_pool": {
        "model1": {
          "column": "task_id",
          "foreign_key": "task"
        },
        "model2": {
          "column": "pool_id",
          "foreign_key": "pool"
        }
      },
      "originals_association": {
        "model1": {
          "column": "original_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "child_id",
          "foreign_key": "service"
        }
      },
      "service_target_device": {
        "model1": {
          "column": "service_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "device_id",
          "foreign_key": "device"
        }
      },
      "service_target_pool": {
        "model1": {
          "column": "service_id",
          "foreign_key": "service"
        },
        "model2": {
          "column": "pool_id",
          "foreign_key": "pool"
        }
      },
      "credential_device": {
        "model1": {
          "column": "credential_id",
          "foreign_key": "credential"
        },
        "model2": {
          "column": "pool_id",
          "foreign_key": "pool"
        }
      },
      "credential_user": {
        "model1": {
          "column": "credential_id",
          "foreign_key": "credential"
        },
        "model2": {
          "column": "pool_id",
          "foreign_key": "pool"
        }
      }
    }
  },
  "private_properties": {
    "service": [
      "custom_password",
      "jump_password",
      "librenms_token",
      "netbox_token",
      "opennms_password",
      "password"
    ],
    "credential": ["enable_password", "password", "private_key"],
    "user": ["password"]
  },
  "dont_serialize": {
    "device": ["configuration", "operational_data", "specialized_data"]
  }
}
